<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 
    
    <bean id = "shape_circle" class = "com.oocl.licupca.sw.exercise5.Shape">
		<property name = "Shape">
			<bean  id = "circle" class = "com.oocl.licupca.sw.exercise5.Circle"/>
		</property>
    </bean>
    
    <bean id = "shape_triangle" class = "com.oocl.licupca.sw.exercise5.Shape">
		<property name = "Shape">
			<bean  id = "triangle" class = "com.oocl.licupca.sw.exercise5.Triangle"/>
		</property>
    </bean>
    
</beans>