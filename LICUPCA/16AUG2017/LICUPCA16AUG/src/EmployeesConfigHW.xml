<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="company" class="com.oocl.licupca.hw.Employee">
		<property name="company" value="Jollibee" />
	</bean>

	<bean id="employee_pedro" class="com.oocl.licupca.hw.Employee" parent = "company">
		<property name="name" value="Pedro" />
		<property name="role" value="Associate Programmer" />
	</bean>
	
	<bean id="employee_juan" class="com.oocl.licupca.hw.Employee" parent = "company">
		<property name="name" value="Juan" />
		<property name="role" value="Business Analyst" />
	</bean>
</beans>