<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="employee_juan" class="com.oocl.licupca.hw.Employee">
		<property name="name" value="Juan" />
		<property name="role" value="Admin" />
	</bean>

	<bean id="employee_jose" class="com.oocl.licupca.hw.Employee">
		<property name="name" value="Jose" />
		<property name="role" value="Guest" />
	</bean>
	
	<bean id="employee_pedro" class="com.oocl.licupca.hw.Employee">
		<property name="name" value="Pedro" />
		<property name="role" value="Congressman" />
	</bean>
</beans>