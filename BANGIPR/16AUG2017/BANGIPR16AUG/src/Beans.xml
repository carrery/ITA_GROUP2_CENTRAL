<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
   
   
    <bean id="shapeCirc" class = "com.oocl.bangipr.sw5.Shape">
		<property name = "shape">
			<bean id = "circle" class = "com.oocl.bangipr.sw5.Circle">
			</bean>
		 </property>
	</bean>
	
	<bean id="shapeTria" class = "com.oocl.bangipr.sw5.Shape">
		<property name = "shape">
			<bean id= "triangle" class = "com.oocl.bangipr.sw5.Traingle">
			</bean>
		 </property>
	</bean>
		
	 <bean id = "employeeLists" class = "com.oocl.bangipr.sw6.Employee">

      <property name = "employeeList">
         <list>
            <value>Alice</value>
            <value>Programmer</value>
            <value>1000321</value>
         </list>
         
      </property>
   
   </bean>
   
   <bean id="order" class = "com.oocl.bangipr.hw1.Order">
		<property name = "order">
   			<bean class="com.oocl.bangipr.hw1.ClassA">
   				<property name = "item" value= "Burger"/>
   				<property name = "price" value = "$10"/>
   				<property name = "address" value = "Philippines"/>
   			</bean>
		</property>
	</bean>

</beans>   