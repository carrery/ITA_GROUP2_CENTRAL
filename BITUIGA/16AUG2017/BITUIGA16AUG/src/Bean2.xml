<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="empParentClass" class="com.oocl.bituiga.hw3.Employee">
        <property name="company" value="CUTIE COMPANY" />
    </bean>
    <bean id="thisEmployee" parent="empParentClass">
        <property name="empId" value="001" />
        <property name="name" value="President MUMU" />
    </bean>
</beans>